{
  "platform": "django",
  "django": {
    "pythonVersion": "3.12",
    "collectStatic": true,
    "compileMessages": false,
    "modifySettings": true,
    "timezone": "Asia/Tehran"
    
  },
  "app": "navidyar",
  "disks": [
    {
      "name": "static",
      "mountTo": "static"
    },
    {
      "name": "media",
      "mountTo": "media"
    }
  ],
  "build": {
    "commands": [
      "python manage.py migrate auth admin contenttypes sessions sites taggit accounts blog captcha contact core robots --noinput"
    ]
  },
  "skip": {
    "commands": [
      "makemigrations"
    ]
  }
}