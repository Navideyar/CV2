{
  "platform": "django",
  "django": {
    "pythonVersion": "3.12",
    "collectStatic": true,
    "compileMessages": false,
    "modifySettings": true,
    "timezone": "Asia/Tehran",
    "settingsFile": "cv2/setting/production.py",
    "wsgi": "cv2.wsgi",
    "cmd": "gunicorn cv2.wsgi --bind 0.0.0.0:8000"
  },
  "app": "navidyar",
  "disks": [
    {
      "name": "static",
      "mountTo": "static"
    },
    {
      "name": "media",
      "mountTo": "media"
    }
  ]
}